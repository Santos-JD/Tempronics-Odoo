<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <data>
        <record model='ir.ui.view' id='wizard_form_temp_re_stock_loc'>
            <field name="name">wizard.temp.report.stock.location.form</field>
            <field name="model">wizard.temp.report.stock.location</field>
            <field name="arch" type="xml">
                <form string="Wizard">
                        <group string="Location">
                            <field name="location" widget="many2many_tags"/>
                        </group>
                        <notebook>
                            <page string="Category">
                                <field name="category">
                                    <tree>
                                        <field name="name"/>
                                    </tree>
                                </field>
                            </page>
                        </notebook>
                    <footer>
                        <button name="export_xls" type="object" default_focus="1"
                                string="Export Product with Stock Info" class="oe_highlight"
                                context="{'xls_export':1}" icon="fa-download"/>
                        <button string="Cancel" class="oe_link" special="cancel" />
                    </footer>
                </form>
            </field>
        </record>

        
        
    </data>
</odoo>